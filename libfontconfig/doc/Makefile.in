	$(mkinstalldirs) $(DESTDIR)$(DOCDIR)
	for i in $(DOC_FILES); do \
	  echo '-- Installing '$$i ; \
	  $(INSTALL_DATA) $$i $(DESTDIR)$(DOCDIR)/$$i; \
	done
	for i in $(DOC_DIRS); do \
	  $(mkinstalldirs) $(DESTDIR)$(DOCDIR)/$$i ; \
	  for f in $$i/*; do \
	    echo '-- Installing '$$f ; \
	    $(INSTALL_DATA) $$f $(DESTDIR)$(DOCDIR)/$$f; \
	  done \
	done

